    "@prisma/adapter-neon": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "@types/qrcode": "^1.5.6",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
