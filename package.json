    "html5-qrcode": "^2.3.8",
    "jspdf": "^3.0.4",
    "lucide-react": "^0.555.0",
    "next": "16.0.5",
    "next-auth": "^5.0.0-beta.30",
    "nodemailer": "^7.0.11",
    "openai": "^6.9.1",
    "qrcode": "^1.5.4",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-player": "^3.4.0",
    "ws": "^8.18.3",
    "youtube-transcript": "^1.2.1",
    "zod": "^4.1.13"
